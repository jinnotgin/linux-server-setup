version: "3.9"
services:
  xray:
    image: teddysun/xray:latest
    container_name: xray-vless-ws
    restart: unless-stopped
    volumes:
      - ./config.json:/etc/xray/config.json:ro
    networks:
      - proxy_net
    command: ["xray", "run", "-config", "/etc/xray/config.json"]

networks:
  proxy_net:
    external: true
